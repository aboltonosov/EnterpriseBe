// 4D:ERP для Беларуси, Юлия, 24.11.2017 9:32:41 
// Поступление безналичных ДС, № 16659
// {
Функция СформироватьПлатежноеТребование(МассивОбъектов, ОбъектыПечати)
	
	Макет = ПолучитьМакет("ПФ_MXL_ПлатежноеТребование");
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	ПоступлениеБезналичныхДенежныхСредств.Организация КАК ТекстПлательщика,
	|	ПоступлениеБезналичныхДенежныхСредств.БанковскийСчет,
	|	ПоступлениеБезналичныхДенежныхСредств.Контрагент КАК Получатель,
	|	ПоступлениеБезналичныхДенежныхСредств.БанковскийСчетКонтрагента,
	|	ПоступлениеБезналичныхДенежныхСредств.СуммаДокумента КАК СуммаЧислом,
	|	ПоступлениеБезналичныхДенежныхСредств.Валюта КАК ВалютаДокумента,
	|	ПоступлениеБезналичныхДенежныхСредств.НазначениеПлатежа,
	|	ПоступлениеБезналичныхДенежныхСредств.Валюта.Код КАК КодВалюты,
	|	ПоступлениеБезналичныхДенежныхСредств.НомерВходящегоДокумента КАК НаименованиеНомер,
	|	ПоступлениеБезналичныхДенежныхСредств.ДатаВходящегоДокумента КАК ДатаДокумента,
	|	ПоступлениеБезналичныхДенежныхСредств.ТипПлатежногоДокумента
	|ИЗ
	|	Документ.ПоступлениеБезналичныхДенежныхСредств КАК ПоступлениеБезналичныхДенежныхСредств
	|ГДЕ
	|	ПоступлениеБезналичныхДенежныхСредств.Ссылка В(&МассивДокументов)
	|	И ПоступлениеБезналичныхДенежныхСредств.ТипПлатежногоДокумента = &ТипПлатежногоДокумента";
	
	ТипПлатежногоДокумента = Перечисления.ТипыПлатежныхДокументов.ПлатежноеТребование;
	Запрос.УстановитьПараметр("ТипПлатежногоДокумента", ТипПлатежногоДокумента);
	Запрос.УстановитьПараметр("МассивДокументов", МассивОбъектов);
	
	ТабДокумент = Новый ТабличныйДокумент;
	ТабДокумент.ИмяПараметровПечати = "ПАРАМЕТРЫ_ПЕЧАТИ_ПлатежноеТребование_ПлатежноеТребование";

	Выборка = Запрос.Выполнить().Выбрать();
	Пока Выборка.Следующий() Цикл
		
		Обл = Макет.ПолучитьОбласть("ЗаголовокТаблицы");
		
		Обл.Параметры.Заполнить(Выборка);
		
		Обл.Параметры.НомерСчетаПлательщика = Выборка.БанковскийСчет.НомерСчета;
		Обл.Параметры.ДатаДокумента = Формат(Выборка.ДатаДокумента, "ДФ=dd.MM.yyyy");
		Обл.Параметры.НаименованиеБанкаПлательщика = Выборка.БанковскийСчет.Банк;
		Обл.Параметры.БИКБанкаПлательщика = Выборка.БанковскийСчет.Банк.Код;
		Обл.Параметры.НаименованиеБанкаПолучателя = Выборка.БанковскийСчетКонтрагента.Банк;
		Обл.Параметры.БИКБанкаПолучателя = Выборка.БанковскийСчетКонтрагента.Банк.Код;
		Обл.Параметры.ИННПлательщика = Выборка.ТекстПлательщика.ИНН;
		Обл.Параметры.ИННПолучателя = Выборка.Получатель.ИНН;
		Обл.Параметры.СуммаПрописью = РаботаСКурсамиВалют.СформироватьСуммуПрописью(
				Выборка.СуммаЧислом,
				Выборка.ВалютаДокумента,
				Истина);
		
		
		ТабДокумент.Вывести(Обл);
	КонецЦикла;
	Возврат ТабДокумент;

КонецФункции
