	ОкончаниеЗамера = ТекущаяУниверсальнаяДатаВМиллисекундах();
	ОписаниеГрафа = ОписаниеГрафа + "
		|	" + СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
					НСтр("ru='Время подготовки графа: %1 сек.'"),
