		+ ТекстОписаниеДанныхДляПартийПроизводства()
		+ "ОБЪЕДИНИТЬ ВСЕ" + ТекстОстаткиПартийПроизводства() // использует ПрошлыеПоступления
		+ "ОБЪЕДИНИТЬ ВСЕ" + ТекстПервичныеПартииПроизводства()
