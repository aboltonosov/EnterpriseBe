	Если Не ЭтоОбъектРасчетов Тогда ТекстЗачетАвансовКлиентов = "";
	Иначе
		ТекстЗачетАвансовКлиентов = " ОБЪЕДИНИТЬ ВСЕ " + "
		|ВЫБРАТЬ //// Зачет авансов клиентов (Дт <62.2> :: Кт <62.1, 76>)
