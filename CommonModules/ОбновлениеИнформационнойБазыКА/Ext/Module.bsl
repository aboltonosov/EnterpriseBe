// Обработчик обновления КА
// Установка служебной константы использования управления производством 2.1.
//
Процедура УстановитьИспользованиеПроизводства21(Параметры) Экспорт
	
	ОбработкаЗавершена = Истина;
	
	Если ПолучитьФункциональнуюОпцию("КомплекснаяАвтоматизация") Тогда
		
		НачатьТранзакцию();
		Попытка
			
			Блокировка = Новый БлокировкаДанных();
			
			ЭлементБлокировки = Блокировка.Добавить("Константа.ИспользоватьПроизводство");
			ЭлементБлокировки = Блокировка.Добавить("Константа.ИспользоватьУправлениеПроизводством");
			
			Блокировка.Заблокировать();
			
			ЗначениеКонстанты = Константы.ИспользоватьПроизводство.Получить();
			Константы.ИспользоватьУправлениеПроизводством.Установить(ЗначениеКонстанты);
			
			ЗафиксироватьТранзакцию();
			
		Исключение
			
			ОбработкаЗавершена = Ложь;
			
			ОтменитьТранзакцию();
			
			Шаблон = НСтр("ru = 'Не удалось установить значение константы Использование производства (2.1) по причине: %1'");
			ТекстСообщения = СтрШаблон(Шаблон, ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			
			ЗаписьЖурналаРегистрации(ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
				УровеньЖурналаРегистрации.Предупреждение,
				Метаданные.Константы.ИспользоватьУправлениеПроизводством,
				,
				ТекстСообщения);
				
		КонецПопытки;
		
	КонецЕсли;
	
	Параметры.ОбработкаЗавершена = ОбработкаЗавершена;
	
КонецПроцедуры

