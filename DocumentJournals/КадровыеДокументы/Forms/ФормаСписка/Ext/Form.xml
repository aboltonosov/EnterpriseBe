									<dcsset:comparisonType>Equal</dcsset:comparisonType>
									<dcsset:right xsi:type="dcscor:Field">БронированиеПозиции</dcsset:right>
								</dcsset:item>
							</dcsset:filter>
							<dcsset:appearance>
